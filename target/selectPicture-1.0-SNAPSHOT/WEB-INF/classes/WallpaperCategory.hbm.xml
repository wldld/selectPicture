<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yz.Entity.WallpaperCategory" table="wallpaper_category" schema="wallpaper">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="wallpaperId">
            <column name="wallpaper_id" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="categoryId">
            <column name="category_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>