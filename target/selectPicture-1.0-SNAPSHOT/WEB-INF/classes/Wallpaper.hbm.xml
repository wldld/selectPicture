<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yz.Entity.Wallpaper" table="wallpaper" schema="wallpaper">
        <id name="id">
            <column name="id" sql-type="varchar(60)" length="60"/>
        </id>
        <property name="thumbUrl">
            <column name="thumb_url" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="imgUrl">
            <column name="img_url" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="previewUrl">
            <column name="preview_url" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="tag">
            <column name="tag" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>